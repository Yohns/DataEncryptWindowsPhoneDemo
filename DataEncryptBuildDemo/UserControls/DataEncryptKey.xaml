<UserControl x:Class="DataEncryptBuildDemo.UserControls.DataEncryptKey"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="480" d:DesignWidth="480">
    
    <Grid x:Name="LayoutRoot">
        <StackPanel Margin="20">
            <TextBlock Text="HMAC_MD5 加密Key:" Margin="0,15,0,0"></TextBlock>
            <TextBox x:Name="DataEncryptKey_TB" Margin="-10,20,0,0" ></TextBox>
            
            <StackPanel Orientation="Horizontal">
                <Button x:Name="SetEncryptKey_BT" Content="设    置" Width="200" Margin="-10,20,0,0" Click="SetEncryptKey_BT_Click"></Button>
                <Button x:Name="CancelEncryptKey_BT" Content="取    消" Width="200" Margin="50,20,0,0" Click="CancelEncryptKey_BT_Click" > </Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
